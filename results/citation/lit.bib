 isbn = {9781107159983},
 editor = {Davis, Richard and Taras, David},
 booktitle = {Justices and journalists},
 year = {2017},
 address = {Cambridge, United Kingdom},
 doi = {10.1017/9781316672228.005},
 file = {09.0{\_}pp{\_}81{\_}100{\_}The{\_}Uncomfortable{\_}Embrace{\_}The{\_}Supreme{\_}Court{\_}and{\_}the{\_}Media{\_}in{\_}Canada:Attachments/09.0{\_}pp{\_}81{\_}100{\_}The{\_}Uncomfortable{\_}Embrace{\_}The{\_}Supreme{\_}Court{\_}and{\_}the{\_}Media{\_}in{\_}Canada.pdf:application/pdf}
